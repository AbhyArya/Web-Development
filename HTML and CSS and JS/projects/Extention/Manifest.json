{
    "name":"color picker",
    "description": "A color picker extention",
    "version":"1.0",
    "manifest_version": 3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "activeTab",
        "scripting"
    ],
    "action":{
        "default_popup":"popup.html"
    }, 
    "icons":{
        "16":"./icons8-color-picker-48.png",
        "32":"./icons8-color-picker-48.png",
        "48":"./icons8-color-picker-96.png"
    }
}